task touchff(){
  while(true){
  int val = Sensor(TOUCH_PORT);
  ClearScreen();
  TextOut(0, LCD_LINE5, "Touch: "+NumToStr(val));
  Wait(1500);
  }
}

// TOUCH : 1 when pressed


